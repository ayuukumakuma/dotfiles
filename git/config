# script/update-git-config-from-1password.sh でユーザー情報を更新する
[alias]
	s = switch
  st = stash
  a = add
  c = commit
  p = pull
  ps = push
[color]
	ui = true
[pull]
	ff = only
[init]
	defaultBranch = main
[gpg]
	format = ssh
[gpg "ssh"]
	program = /Applications/1Password.app/Contents/MacOS/op-ssh-sign
[commit]
	gpgsign = true
